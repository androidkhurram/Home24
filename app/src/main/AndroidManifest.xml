<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.neberox.app.survey"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.home24.task.survey.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.home24.task.survey.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.home24.task.survey.SurveyActivity">
        </activity>

        <activity android:name="com.home24.task.survey.ResultActivity"
            android:label="Review">
        </activity>


    </application>

</manifest>